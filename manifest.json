{
  "name": "chrome url blocker",
  "version": "1.0",
  "description": "Chrome URL Blocking Extension",
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "manifest_version": 3
}
